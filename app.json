{
  "expo": {
    "name": "My ToDo",
    "slug": "MyToDo",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/todo.png"
      },
      "splash": {
        "image": "./assets/todo.png",
        "backgroundColor": "#DC133B"
      },
      "package": "com.safersunny.todolist"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
